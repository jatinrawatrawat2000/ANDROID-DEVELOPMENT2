<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/z"
    tools:context=".lowlungs">

    <Button
        android:id="@+id/button8"
        android:layout_width="111dp"
        android:layout_height="62dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:onClick="back"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="47dp"
        android:text="Back!"
        app:backgroundTint="#A56D1C" />

    <TextView
        android:id="@+id/text_view_co"
        android:layout_width="155dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"

        android:layout_centerInParent="false"
        android:layout_marginEnd="135dp"

        android:layout_marginBottom="431dp"
        android:text="00.00"
        android:textAlignment="center"
        android:textSize="34sp" />

    <Button
        android:id="@+id/button_start_pa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="303dp"
        android:layout_marginBottom="349dp"
        android:text="Start"
        app:backgroundTint="#A56D1C" />

    <Button
        android:id="@+id/button_r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="351dp"
        android:text="Reset"
        app:backgroundTint="#A56D1C" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="128dp"
        android:layout_height="94dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="516dp"
        app:srcCompat="@drawable/yo" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="341dp"
        android:layout_height="207dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="123dp"
        app:srcCompat="@drawable/lowlung" />

    <Button
        android:id="@+id/button22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="525dp"
        android:onClick="play"
        android:background="@drawable/spek"
        android:text="PLAY!!"
        android:textColor="#090505"
        app:backgroundTint="#A56D1C" />

    <Button
        android:id="@+id/button23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:background="@drawable/spek"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="461dp"
        android:onClick="pause"
        android:text="PAUSE!!"
        android:textColor="#020101"
        app:backgroundTint="#A56D1C" />
</RelativeLayout>