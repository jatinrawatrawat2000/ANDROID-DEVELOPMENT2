<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/z"
    tools:context=".dhanurasana">

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:onClick="back"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="50dp"
        android:text="Back!"
        app:backgroundTint="#A56D1C" />

    <TextView
        android:id="@+id/text_view_common"
        android:layout_width="155dp"
        android:layout_height="56dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"

        android:layout_centerInParent="false"
        android:layout_marginEnd="135dp"

        android:layout_marginBottom="431dp"
        android:text="00.00"
        android:textAlignment="center"
        android:textSize="34sp" />

    <Button
        android:id="@+id/button_start_p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="303dp"
        android:layout_marginBottom="349dp"
        android:text="Start"
        app:backgroundTint="#A56D1C" />

    <Button
        android:id="@+id/button_fig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="351dp"
        android:text="Reset"
        app:backgroundTint="#A56D1C" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="149dp"
        android:layout_height="84dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="514dp"
        app:srcCompat="@drawable/yoga" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="313dp"
        android:layout_height="213dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="114dp"
        app:srcCompat="@drawable/dharu" />

    <Button
        android:id="@+id/but"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="17dp"
        android:background="@drawable/spek"
        android:layout_marginBottom="534dp"
        android:text="PLAY!!"
        android:textColor="#0C0707"
        app:backgroundTint="#A56D1C" />

    <Button
        android:id="@+id/butt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/spek"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="468dp"
        android:text="PAUSE!!"
        android:textColor="#090606"
        app:backgroundTint="#A56D1C" />
</RelativeLayout>